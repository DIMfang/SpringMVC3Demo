<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>  
    <class name="com.demo.model.models.Project" table="project" lazy="false">  

		<id name="id" column="id" type="integer" >
		  <generator class="native" />
		</id>
		
		
		<property name="projectName" column="projectName" type="string" not-null="true"/>
		<property name="userId" column="userId" type="integer" not-null="true"/>
		<property name="CreateTime" column="CreateTime" type="calendar" not-null="true"/>
		

    </class>  
</hibernate-mapping> 