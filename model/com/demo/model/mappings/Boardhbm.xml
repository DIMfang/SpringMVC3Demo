<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>  
    <class name="com.demo.model.models.Board" table="Board" lazy="false">  

		<id name="id" column="id" type="integer" >
		  <generator class="native" />
		</id>
		
		
		<property name="boardName" column="boardName" type="string" not-null="true"/>
		<property name="projectNum" column="projectNumber" type="integer" not-null="true"/>
		<property name="boardType" column="boardType" type="integer" not-null="true"/>
		<property name="host" column="host" type="string" not-null="true"/>
		

    </class>  
</hibernate-mapping> 